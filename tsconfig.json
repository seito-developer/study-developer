{
	"include": [
		"nextjs-blog/**/*"
	],
	"extends": "nextjs-blog/node_modules",
	"target": "es5",
	"compileOnSave": true,
	"allowJs": true,
	"sourceMap": true,
	"noImplicitAny": true,
	"noImplicitThis": true,
	"alwaysStrict": true,
	"strictPropertyInitialization": true,
	"baseUrl": "./",
	"noUnusedLocals": true,
	"noFallthroughCasesInSwitch": true,
	"forceConsistentCasingInFileNames": true
}